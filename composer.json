{
  "name": "ideo/doctrine-extensions",
  "description": "Doctrine extension library.",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Kouichi Machida",
      "email": "k-machida@aideo.co.jp"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "doctrine/dbal": "^2.5",
    "doctrine/orm": "^2.5",
    "ext-pdo": "*",
    "nesbot/carbon": "^1.2",
    "php": "^5.4|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.7"
  },
  "autoload": {
    "psr-4": {
      "Ideo\\Doctrine\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ideo\\Doctrine\\": "tests"
    }
  }
}
